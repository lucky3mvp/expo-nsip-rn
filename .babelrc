{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    [
      "babel-plugin-module-alias",
      [
        {
          "src": "./src/page/common",
          "expose": "common"
        }, {
          "src": "./src/component",
          "expose": "component"
        }
      ]
    ]
  ]
}
